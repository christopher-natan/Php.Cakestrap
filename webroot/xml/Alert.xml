<?xml version="1.0" encoding="utf-8"?>
<cakestrap>
    <container>
        <![CDATA[
            <div class="alert {{type}} {{dismissible}} {{align}}" role="alert">{{button}} <i class="fa {{icon}}"></i> {{content}}</div>
         ]]>
    </container>
    <button>
        <![CDATA[
            <button type="button" class="close" data-dismiss="alert" aria-label="{{label}}"><span aria-hidden="true">&times;</span></button>
         ]]>
    </button>
</cakestrap>